{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "../**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "articles/**.md",
          "api/**.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "styles/**"
        ]
      }
    ],
    "dest": "_site"
  }
}
